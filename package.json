{
  "name": "network-snatcher",
  "version": "0.1.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "concurrently \"yarn build:client\" \"yarn build:server\"",
    "build:client": "webpack --config src/client/webpack.config.js",
    "build:server": "tsc -p src/server/tsconfig.json",
    "start": "concurrently \"yarn start:client\" \"yarn start:server\"",
    "start:client": "webpack-dev-server --mode development --inline --hot --config src/client/webpack.config.js",
    "start:server": "nodemon"
  },
  "dependencies": {
    "emotion": "^9.2.10",
    "express": "^4.16.3",
    "normalize.css": "^8.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-easy-state": "^6.0.4",
    "react-emotion": "^9.2.10",
    "react-json-view": "^1.19.1",
    "shortid": "^2.2.13",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/react": "^16.4.14",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-plugin-emotion": "^9.2.10",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "style-loader": "^0.23.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  }
}
